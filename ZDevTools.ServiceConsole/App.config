<?xml version="1.0" encoding="utf-8"?>
<!-- 该配置文件由服务壳程序使用，应由各服务类库的app.config文件和原始壳配置文件合并而来 -->
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,log4net" />
    <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="ZDevTools.ServiceConsole.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
      <section name="ZDevTools.ServiceCore.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    </sectionGroup>
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <log4net>
    <appender name="FormAppender" type="ZDevTools.ServiceConsole.FormAppender">
      <layout type="log4net.Layout.PatternLayout">
        <header value="" />
        <footer value="" />
        <conversionPattern value="%date{yyyy-MM-dd HH:mm:ss} %-5level - %message%0.0exception" />
      </layout>
    </appender>
    <appender name="LogFileAppender" type="log4net.Appender.RollingFileAppender">
      <!--最小锁定模型以允许多个进程可以写入同一个文件-->
      <!--<lockingModel type="log4net.Appender.FileAppender+MinimalLock" />-->
      <file value="logs\\console\\log" />
      <!-- Example using environment variables in params -->
      <!-- <file value="${TMP}\log-file.txt" /> -->
      <appendToFile value="true" />
      <param name="MaxSizeRollBackups" value="100" />
      <param name="MaxFileSize" value="10240" />
      <param name="StaticLogFileName" value="false" />
      <param name="RollingStyle" value="Date" />
      <param name="DatePattern" value="_yyyy-MM-dd&quot;.log&quot;" />
      <!-- An alternate output encoding can be specified -->
      <!-- <encoding value="unicodeFFFE" /> -->
      <layout type="log4net.Layout.PatternLayout">
        <header value="&gt;&gt;&gt;&gt;&gt;&gt;&gt;&gt;&gt;&gt;&#xD;&#xA;" />
        <footer value="&lt;&lt;&#xD;&#xA;" />
        <conversionPattern value="%date [%thread] %-5level %logger{1} - %message%newline" />
      </layout>
      <filter type="log4net.Filter.LevelRangeFilter">
        <levelMin value="WARN" />
        <levelMax value="FATAL" />
      </filter>
      <!-- Alternate layout using XML
      <layout type="log4net.Layout.XMLLayout" /> -->
    </appender>
    <root>
      <appender-ref ref="FormAppender" />
      <appender-ref ref="LogFileAppender" />
    </root>
  </log4net>
  <applicationSettings>
    <ZDevTools.ServiceConsole.Properties.Settings>
      <setting name="ServiceConsoleTitle" serializeAs="String">
        <value>服务控制台名称（您可以自定义控制台要显示的名称）</value>
      </setting>
      <setting name="ServiceWaitTimeOut" serializeAs="String">
        <value>600</value>
      </setting>
      <setting name="AutoUpdateUri" serializeAs="String">
        <value></value>
      </setting>
      <setting name="AutoUpdateInterval" serializeAs="String">
        <value>1800</value>
      </setting>
      <setting name="ServicesAssembly" serializeAs="String">
        <value />
      </setting>
    </ZDevTools.ServiceConsole.Properties.Settings>
    <ZDevTools.ServiceCore.Properties.Settings>
      <setting name="RedisServer" serializeAs="String">
        <value></value>
      </setting>
      <setting name="WindowsServiceLogLevel" serializeAs="String">
        <value>All</value>
      </setting>
    </ZDevTools.ServiceCore.Properties.Settings>
  </applicationSettings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="SimpleUpdater" publicKeyToken="c532149a76b9a64b" />
        <bindingRedirect oldVersion="1.0.0.0-99.99.99.99" newVersion="4.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Unity.Abstractions" publicKeyToken="489b6accfaf20ef0" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-5.11.1.0" newVersion="5.11.1.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Unity.Container" publicKeyToken="489b6accfaf20ef0" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-5.11.1.0" newVersion="5.11.1.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Threading.Tasks.Extensions" publicKeyToken="cc7b13ffcd2ddd51" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.2.0.1" newVersion="4.2.0.1" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>
